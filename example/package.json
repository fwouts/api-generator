{
  "name": "example",
  "scripts": {
    "build": "tsc",
    "server": "ts-node src/server.ts",
    "client": "ts-node src/client.ts",
    "generate": "yarn generate-client && yarn generate-server",
    "generate-client": "ts-node ../src/cli.ts generate typescript users.api -c https://api.test.com > src/client.ts",
    "generate-server": "ts-node ../src/cli.ts generate typescript users.api -s > src/server.ts"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.11.1",
    "axios": "^0.18.0",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "ts-node": "^5.0.1",
    "typescript": "^2.8.1"
  }
}
